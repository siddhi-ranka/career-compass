# Copy to .env and fill values
# URL of the generative API (e.g., Vertex AI Generative API or another Gemini-compatible endpoint)
GENERATIVE_API_URL=https://your-generative-api.example/v1/models/gemini-1:predict
# API key or Bearer token to authorize requests to the generative API
GENERATIVE_API_KEY=your_api_key_here
# Optional: generative provider hint (e.g. 'vertex' or 'generic')
GENERATIVE_PROVIDER=vertex
# Backend port
PORT=4000
